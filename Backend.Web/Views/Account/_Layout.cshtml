@using System.Web.Optimization
@using Abp.Web.Mvc.Extensions

@{
    SetAntiForgeryCookie();
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Backend | Log in</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    @Styles.Render("~/Bundles/vendor/css")

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Content/adminlte/plugins/html5shiv/html5shiv.min.js")"></script>
    <script src="@Url.Content("~/Content/adminlte/plugins/respond/respond.min.js")"></script>
    <![endif]-->
    @RenderSection("Styles", false);
</head>
<body class="hold-transition login-page">
    <div class="login-box">
        <div class="login-logo">
            <a href="/"><b>Backend</b></a>
        </div>
        <!-- /.login-logo -->
        @RenderBody()
    </div>
    <!-- /.login-box -->
    
    @Scripts.Render("~/Bundles/vendor/js/bottom")

    @RenderSection("Scripts", false);

</body>
</html>

